{
  "name": "torrent-player",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Andro999b/torrent-player"
  },
  "main": "index.js",
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ],
    "delay": "2500"
  },
  "scripts": {
    "profile": "webpack --mode production --profile --json --config client/webpack.config.js > stats.json",
    "lint": "eslint .",
    "start": "concurrently -n \"SERVER,CLIENT\" -c \"black.bold,blue.bold\" \"npm run server\" \"npm run client\"",
    "debug": "DEBUG=transcode-hls concurrently -n \"SERVER,CLIENT\" -c \"black.bold,blue.bold\" \"npm run server\" \"npm run client\"",
    "server": "nodemon --inspect index.js",
    "client": "webpack-dev-server --config client/webpack.dev.config.js",
    "build": "webpack --config client/webpack.config.js --mode production",
    "build-dev": "webpack --config client/webpack.config.js --mode development"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "eslint": "^5.3.0",
    "eslint-plugin-react": "^7.10.0",
    "html-webpack-plugin": "^3.2.0",
    "mobx-react-devtools": "^6.0.3",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "^4.16.5",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@material-ui/core": "^3.0.2",
    "@material-ui/icons": "^3.0.1",
    "bidirectional-map": "^1.0.4",
    "cheerio": "^1.0.0-rc.2",
    "classnames": "^2.2.6",
    "concat-stream": "^1.6.2",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "file-size": "^1.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^7.0.0",
    "hls.js": "^0.11.0",
    "jstoxml": "^1.4.3",
    "lodash.debounce": "^4.0.8",
    "lodash.groupby": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "lowdb": "^1.0.0",
    "m3u8-parser": "^4.2.0",
    "memoize-one": "^4.0.2",
    "mime-types": "^2.1.19",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
    "node-ssdp": "^4.0.0",
    "parse-torrent": "^6.1.1",
    "react": "^16.4.2",
    "react-autosuggest": "^9.3.4",
    "react-dom": "^16.4.2",
    "react-full-screen": "^0.2.2",
