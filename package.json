{
  "name": "torrent-player",
  "version": "1.0.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Andro999b/torrent-player"
  },
  "main": "index.js",
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ],
    "delay": "2500"
  },
  "scripts": {
    "start": "concurrently -n \"SERVER,CLIENT\" -c \"black.bold,blue.bold\" \"npm run server\" \"npm run client\"",
    "debug": "DEBUG=transcode-hls concurrently -n \"SERVER,CLIENT\" -c \"black.bold,blue.bold\" \"npm run server\" \"npm run client\"",
    "server": "nodemon --inspect index.js",
    "client": "webpack --watch --config client/webpack.config.js --mode development",
    "build": "webpack --config client/webpack.config.js --mode production"
  },
  "author": "",
  "license": "ISC",
